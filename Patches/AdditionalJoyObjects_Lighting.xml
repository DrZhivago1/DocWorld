<?xml version="1.0" encoding="UTF-8"?>
<Patch>
  <!--Move chandeliers from AJO to the "Lighting" tab-->
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Additional Joy Objects</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <!--Furniture_Chandeliers-->
        <li Class="PatchOperationReplace">
          <xpath>Defs/ThingDef[@Name="ChanPart_Support_Base"]/designationCategory</xpath>
          <value>
            <designationCategory>Lighting</designationCategory>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
