<?xml version="1.0" encoding="UTF-8"?>
<Patch>
  <!--Remove annoying metal sound error logs from Colony Leadership-->
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Weapon Tech</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationTest">
          <xpath>Defs/ThingDef[defName="Apparel_Osprey"]/thingCategories/Apparel</xpath>
          <success>Always</success>
        </li>
        <li Class="PatchOperationRemove">
          <xpath>Defs/ThingDef[defName="Apparel_Osprey"]/thingCategories</xpath>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
