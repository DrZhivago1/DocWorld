<?xml version="1.0" encoding="UTF-8"?>
<Patch>
  <!--Put Industrial Rollers behind Electricity Tech-->
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Industrial Rollers</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationTest">
          <xpath>Defs/ThingDef[defName="MovingRailStraight"]/researchPrerequisites</xpath>
          <success>Invert</success>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs/ThingDef[defName="MovingRailStraight"]</xpath>
          <value>
            <researchPrerequisites>
              <li>Electricity</li>
            </researchPrerequisites>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
