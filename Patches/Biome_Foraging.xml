<Patch>
  <!--This file will contain all patches that alter biome foraging. Requires Vanilla Plants Expanded.-->

  <!--==========
    VANILLA
    ==========-->
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Plants Expanded</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <!--Cold-->
        <li Class="PatchOperationReplace">
          <xpath>/Defs/BiomeDef[defName="BorealForest" or defName="ColdBog"]/foragedFood</xpath>
          <value>
            <foragedFood>VCE_Bearberry</foragedFood>
          </value>
        </li>
        <li Class="PatchOperationReplace">
          <xpath>/Defs/BiomeDef[defName="Tundra"]/foragedFood</xpath>
          <value>
            <foragedFood>VCE_RawCherry</foragedFood>
          </value>
        </li>
        <!--Warm-->
        <li Class="PatchOperationReplace">
          <xpath>/Defs/BiomeDef[defName="TropicalRainforest" or defName="TropicalSwamp"]/foragedFood</xpath>
          <value>
            <foragedFood>VCE_RawBanana</foragedFood>
          </value>
        </li>
        <!--WarmArid-->
        <li Class="PatchOperationReplace">
          <xpath>/Defs/BiomeDef[defName="AridShrubland"]/foragedFood</xpath>
          <value>
            <foragedFood>VCE_RawPricklyPear</foragedFood>
          </value>
        </li>
      </operations>
    </match>
  </Operation>

  <!--==========
    More Vanilla Biomes
    ==========-->
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Plants Expanded</li>
    </mods>
    <match Class="PatchOperationFindMod">
      <mods>
        <li>More Vanilla Biomes</li>
      </mods>
      <match Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
          <li Class="PatchOperationReplace">
            <xpath>/Defs/BiomeDef[defName="ZBiome_AlpineMeadow"]/foragedFood</xpath>
            <value>
              <foragedFood>VCE_RawCherry</foragedFood>
            </value>
          </li>
          <li Class="PatchOperationReplace">
            <xpath>/Defs/BiomeDef[defName="ZBiome_CloudForest"]/foragedFood</xpath>
            <value>
              <foragedFood>VCE_RawPlum</foragedFood>
            </value>
          </li>
          <li Class="PatchOperationReplace">
            <xpath>/Defs/BiomeDef[defName="ZBiome_DesertOasis"]/foragedFood</xpath>
            <value>
              <foragedFood>VCE_RawPricklyPear</foragedFood>
            </value>
          </li>
          <li Class="PatchOperationReplace">
            <xpath>/Defs/BiomeDef[defName="ZBiome_Grasslands"]/foragedFood</xpath>
            <value>
              <foragedFood>VCE_RawApple</foragedFood>
            </value>
          </li>
          <li Class="PatchOperationReplace">
            <xpath>/Defs/BiomeDef[defName="ZBiome_Marsh"]/foragedFood</xpath>
            <value>
              <foragedFood>VCE_RawPeach</foragedFood>
            </value>
          </li>
        </operations>
      </match>
    </match>
  </Operation>

  <!--==========
    Realistic Planets - Fan Update
    ==========-->
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Plants Expanded</li>
    </mods>
    <match Class="PatchOperationFindMod">
      <mods>
        <li>Realistic Planets - Fan Update</li>
      </mods>
      <match Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
          <li Class="PatchOperationReplace">
            <xpath>/Defs/BiomeDef[defName="RRP_Grassland"]/foragedFood</xpath>
            <value>
              <foragedFood>VCE_RawApple</foragedFood>
            </value>
          </li>
          <li Class="PatchOperationReplace">
            <xpath>/Defs/BiomeDef[defName="RRP_Savanna"]/foragedFood</xpath>
            <value>
              <foragedFood>VCE_RawPeach</foragedFood>
            </value>
          </li>
          <li Class="PatchOperationReplace">
            <xpath>/Defs/BiomeDef[defName="RRP_Steppes" or defName="RRP_Permafrost"]/foragedFood</xpath>
            <value>
              <foragedFood>VCE_Bearberry</foragedFood>
            </value>
          </li>
          <li Class="PatchOperationReplace">
            <xpath>/Defs/BiomeDef[defName="RRP_TemperateDesert" or defName="RRP_Oasis"]/foragedFood</xpath>
            <value>
              <foragedFood>VCE_RawPricklyPear</foragedFood>
            </value>
          </li>
        </operations>
      </match>
    </match>
  </Operation>


</Patch>

