<Patch>

   <!--==========
    VANILLA
    ==========-->

   <!--Vanilla Vents Stuffed-->
   <Operation Class="PatchOperationConditional">
      <xpath>/Defs/ThingDef[defName = "Vent"]/costList</xpath>
      <match Class="PatchOperationReplace">
         <xpath>/Defs/ThingDef[defName = "Vent"]/costList</xpath>
         <value>
            <costStuffCount>30</costStuffCount>
            <stuffCategories>
               <li>Metallic</li>
               <li>Woody</li>
            </stuffCategories>
         </value>
      </match>
   </Operation>
   <!--Vanilla Butcher Table Stuffed-->
   <Operation Class="PatchOperationConditional">
      <xpath>/Defs/ThingDef[defName = "TableButcher"]/stuffCategories/Stony</xpath>
      <nomatch Class="PatchOperationAdd">
         <xpath>/Defs/ThingDef[defName = "TableButcher"]/stuffCategories</xpath>
         <value>
            <li>Stony</li>
         </value>
      </nomatch>
   </Operation>

</Patch>