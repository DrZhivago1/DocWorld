    <mods>
      <li>Remote Tech</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationReplace">
          <xpath>Defs/DesignationCategoryDef[defName="rxRemoteTech"]/order</xpath>
          <value>
            <order>275</order>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
